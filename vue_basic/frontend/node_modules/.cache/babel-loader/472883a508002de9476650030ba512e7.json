rom \"@/components/layout/Header\";\nexport default {\n  name: \"DefaultLayout\",\n  components: { Header },\n  props: {\n    scrollTop : {\n      type: Number,\n      default : 0\n    },\n  },\n  data() {\n    return {\n      isChange: false,\n    };\n  },\n  watch: {\n    scrollTop(af,bf){\n      if(af > bf){\n        this.isChange = true;\n      }\n    }\n  },\n  computed : {\n    headerClass(){\n      if(!this.isChange) return \"\";\n      else{\n        return this.scrollTop > 0 ? \"stickyon\" : \"stickyoff\"\n      }\n    }\n  },\n  methods : {\n  }\n};\n</script>\n"]}]}